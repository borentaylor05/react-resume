{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["react", "jsx-a11y"],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "class-methods-use-this": [0],
        "indent": ["error", 4],
        "comma-dangle": [0],
        "react/prop-types": 0
    }
}